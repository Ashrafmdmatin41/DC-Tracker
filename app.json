{
    "name": "DC Tracker Bot",
    "description": "A bot to find Telegram DC ID and other advanced user information",
    "repository": "https://github.com/BXBotz/DC-Tracker",
    "keywords": [
        "telegram",
        "user",
        "dc_id",
        "data_center",
        "finder",
        "pyrogram",
        "bot"
    ],
    "env": {
        "BOT_TOKEN": {
            "description": "Your Bot token from @BotFather",
            "required": true
        },
        "API_ID": {
            "description": "Your API_ID from https://my.telegram.org/apps",
            "required": true
        },
        "API_HASH": {
            "description": "Your API_HASH from https://my.telegram.org/apps",
            "required": true
        },
        "UPDATE_CHANNEL": {
            "description": "Your Channel Username Without @",
            "required": true,
            "value": "BX_Botz"
        }
    },
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ]
}
